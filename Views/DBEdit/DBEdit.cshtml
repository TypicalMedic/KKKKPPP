@model DBEditViewModel
<h1 class="row mt-3">Изменение БД</h1>
<div class="row mb-2">
    <form class="row mt-2 mb-2">
        <h5 id="1">Какие данные изменить</h5>

        @foreach (var x in Model.allEntities)
        {<div class="col-lg-3">
                <a class="nav-link active" aria-current="page" href="/DBEdit/@x">@x</a>
            </div>
        }
    </form>
</div>