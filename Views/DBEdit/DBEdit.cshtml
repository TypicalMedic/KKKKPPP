@model DBEditViewModel
<h3 class="row mt-3">Изменение БД</h3>
<div class="row mb-2">
    <form class="row mt-2 mb-2">
        <h6 id="1">Какие данные изменить</h6>

        @foreach (var x in Model.allEntities.Where(e => !e.Contains("Связь_") && e != "Страна"))
        {<div class="col-lg-3">
                <a class="nav-link active" aria-current="page" href="/DBEdit/@x">@x.Replace('_', ' ')</a>
            </div>
        }
    </form>
</div>