@model DBAddViewModel
<h3 class="row mt-3">Добавление в БД</h3>
<div class="row mb-2">
    <form class="row mt-2 mb-2">
        <div>
            <h6 id="1">Какой элемент добавить</h6>
        </div>
        

        @foreach (var x in Model.allEntities)
        {<div class="col-lg-3">
                <a class="nav-link active" aria-current="page" href="/DBAdd/@x">@x.Replace('_', ' ')</a>
            </div>
        }
       
    </form>
</div>